{
	"name": "stevebauman/platform-logs",
	"type": "platform-extension",
	"description": "A site log manager",
	"authors": [
		{
			"name": "Steve Bauman",
			"email": "steven_bauman@outlook.com"
		}
	],
    "license": "MIT",
	"repositories": [
		{
			"type": "composer",
